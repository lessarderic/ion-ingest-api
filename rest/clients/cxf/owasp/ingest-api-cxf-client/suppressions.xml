<!--
    Contains the suppressions that can be inherited by dependent projects.

    IMPORTANT: Only suppress vulnerabilities that can NEVER be applicable in dependent projects!
-->
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.2.xsd">
    <suppress>
        <!--
            OWASP infers that jackson-databind-nullable 0.1.0 is related to Jackson 0.1.0
            dependencies and flags vulnerabilities on cpe:2.3:a:fasterxml:jackson:0.1.0 and
            cpe:2.3:a:fasterxml:jackson-databind:0.1.0, which do not exist.
        -->
        <notes><![CDATA[
        file name: jackson-databind-nullable-0.1.0.jar
        ]]></notes>
        <gav regex="true">^org\.openapitools:jackson-databind-nullable:0.1.0$</gav>
        <cpe>cpe:/a:fasterxml:jackson:0.1.0</cpe>
        <cpe>cpe:/a:fasterxml:jackson-databind:0.1.0</cpe>
    </suppress>
</suppressions>
